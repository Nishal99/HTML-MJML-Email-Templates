      <mj-column width="33.3%" vertical-align="middle">
        <mj-image src="https://images.tcgms.net/assets/p29y30j2ck28i00/media/images/9d174f83-a267-4eea-87b6-7f92cbe38d89.png" width="100px" height="auto" padding="0px 0px 20px" href="#" target="_blank" alt="Place 3 hotel logo" css-class="hotel-logo m-p-lr-0"/>
