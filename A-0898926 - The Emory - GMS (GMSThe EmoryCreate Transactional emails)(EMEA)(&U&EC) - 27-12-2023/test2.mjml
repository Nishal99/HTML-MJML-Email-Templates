<mjml>
  <mj-head>
    <mj-title>Your reservation @@confirmation_number@@ has been confirmed.
    </mj-title>
    <mj-preview>
    </mj-preview>
    <mj-font name="Nunito Sans" href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap"/>
    <mj-font name="Forum" href="https://fonts.googleapis.com/css2?family=Forum&display=swap"/>
    <mj-style>
      .hide {
      display:none!important;
      }
      .mob-visi{
      display:none;
      }
      @media (max-width: 595px) {
      div, td, img, table{
      text-align:center !important;
      margin:0 auto;
      }
      .wrp_pad-remove-pd td,
      div.wrp_pad td,div.wrp_pad td td td{
      padding-left:0px !important;
      padding-right:0px !important;
      }
      div.wrp_pad td td{
      padding-left:20px !important;
      padding-right:20px !important;
      }
      td.upgrd-rate{
      padding-top:0px !important;
      }
      div.sec-pad-upgrd td{
      padding-left:0px !important;
      padding-right:0px !important;
      }
      div.sec-pad-upgrd td td{
      padding-left:20px !important;
      padding-right:20px !important;
      }
      tr.social-el td td img{
      padding-left:0px !important;
      padding-right:0px !important;
      }
      td.rm-pd{
      padding-top:0px !important;
      }
      td.rm-pd,
      td.view_brows{
      padding-top:0px !important;
      }
      .div-dec,
      td.header-logo,
      td.social-op{
      padding-bottom:10px!important;
      }
      .rm-top-pd,
      .header-logo{
      padding-top:10px!important;
      }
      .space-mob-none,
      .br-mob{
      display:none !important;
      }
      .pd-bottom-text{
      padding-bottom:0px!important;
      }
      .mob-visi{
      display:block;
      }
      .tbl-bg-img table {
      background-image: none !important;
      }
      .mob-none{
      padding:10px !important;
      }
      .mb-color-title div{
      color:#40aab6 !important;
      }
      .mb-color div{
      color:#002a45 !important;
      }
      .wrp_pad-mob td{
      background-color:#D9D9D6 !important;
      }
      td.mob-visi table {
      width: 100% !important;
      }
      td.btn {
      padding-bottom: 10px !important;
      }
      td.hotel-logo {
      padding-bottom: 20px !important;
      }
      }
    </mj-style>
    <mj-attributes>
      <mj-text align="center">
      </mj-text>
      <mj-section background-color="#fff">
      </mj-section>
    </mj-attributes>
   
  </mj-head>
  <mj-body background-color="#ffffff" width="640px">
    <!--======Pre Header======-->
    <mj-wrapper padding="0px 20px" background-color="#ffffff" css-class="wrp_pad">
      <mj-section background-color="#ffffff" padding="0px">
        <mj-column width="50%">
          <mj-text font-family="Nunito Sans, sans-serif" font-size="16px" padding="12px" line-height="14px" color="#002a45" align="left">
            Your stay at The BoTree
          </mj-text>
        </mj-column>
        <mj-column width="50%">
          <mj-text font-family="Nunito Sans, sans-serif" font-size="12px" padding="12px" line-height="14px" color="#002a45" align="right" css-class="view_brows">
            <a href="https://tcgms.net/tr/c/@@MID@@/2502976/%20@@MID64@@" target="_blank" style="text-decoration:none;color:#002a45;">
              View in browser
            </a>
          </mj-text>
        </mj-column>
      </mj-section>
    </mj-wrapper>
    <!--======Logo & Social icons======-->
    <mj-wrapper padding="0px 20px" background-color="#D9D9D6" css-class="wrp_pad-mob">
      <mj-section background-color="#D9D9D6" padding="20px 40px">
        <mj-column vertical-align="middle">
          <mj-image src="https://images.tcgms.net/assets/p29y30j2ck28i00/media/images/BoTree_Pomegranate-svg_(1)_1.png" width="180px" height="auto" padding="0px" href="#" target="_blank" alt="The botree hotel logo" css-class="header-logo"/>
        </mj-column>
      </mj-section>
    </mj-wrapper>
    <!--Banner image-->
    <mj-section padding="0px 0px 0px" background-color="#D9D9D6">
      <mj-column>
        <mj-image src="https://images.tcgms.net/assets/p29y30j2ck28i00/media/images/Banner_Image_550x300px-min.png" width="540px" height="auto" alt="Interior view of a bedroom" padding="0px" href="#" target="_blank" fluid-on-mobile="true" css-class="banner image"/>
      </mj-column>
    </mj-section>
    <!--======Body Content======-->
    <mj-wrapper padding="0px 20px" background-color="#DBCDD3" css-class="wrp_pad">
      <mj-section padding="0px 30px" background-color="#DBCDD3">
        <mj-column>
          <mj-text font-family="Forum, sans-serif" font-size="46px" padding="25px 0px 0px" line-height="50px" color="#40aab6" text-transform="uppercase" font-weight="900">
            YOUR STAY
          </mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="0px 0px" background-color="#DBCDD3">
        <mj-column>
          <mj-divider width="120px" padding="10px 0px 0px" border-width="1px" border-color="#40aab6"/>
        </mj-column>
      </mj-section>
     
              <mj-section padding="0px 30px" background-color="#DBCDD3">
        <mj-column>
          <mj-text font-family="Nunito Sans, sans-serif" font-size="13px" padding="25px 0px 25px" line-height="150%" color="#002a45" background-color="#DBCDD3">Dear
            <b> @@rsfname@@ @@rslname@@
            </b>
            <br>
            <br>
            You are coming to 
            <a style="text-decoration:none;color:#002a45">stay on 
              <b>@@check_in@@
              </b></a>
            <b>.&nbsp;
            </b>Hope you are excited, we certainly are.
            <div>The BoTree turns guests into neighbourhood familiars and locals into friends.
            </div>
            <div>We are glad you are coming to experience this. 
              <br>
              <br>
              Please take a minute to check everything is as it should be.
            </div>
             
          </mj-text>
        </mj-column>
      </mj-section>
    </mj-wrapper>
    <!--======Your stay details======-->
    <mj-wrapper padding="0px 20px" background-color="#D9D9D6" css-class="wrp_pad">
      <mj-section padding="0px" background-color="#D9D9D6">
        <mj-column>
          <mj-text font-family="Forum, sans-serif" font-size="28px" padding="25px 0px 0px" line-height="28px" color="#002a45" font-weight="600" text-transform="uppercase">
            YOUR STAY DETAILS
          </mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="0px 20px 0px" background-color="#D9D9D6" css-class="div-dec">
