<mjml>
  <mj-head>
    <mj-title>Sublimis Boutique Hotel Camogli Reservation Confirmation
    </mj-title>
    <mj-preview>
    </mj-preview>
    <mj-font name="Libre Baskerville" href="https://fonts.googleapis.com/css2?family=Libre+Baskerville:ital,wght@0,400;0,700;1,400&display=swap"/>
    <mj-font name="Waterfall" href="https://fonts.googleapis.com/css2?family=Waterfall&display=swap"/>
    <mj-style>
      @media (max-width: 595px) {
      div{
      text-align:center !important;
      }
      .header-logo table,
      .ftr-logo table{
      margin:0 auto !important;
      }
      .col-image table{
      width:100% !important;
      }
      td.col-image{
      padding-left:20px !important;
      padding-right:20px !important;
      }
      .social_tbl {
      padding-top: 0px !important;
      text-align: center;
      }
      .btn-pd{
      padding-top:0px !important;
      padding-bottom:20px !important;
      }
      .mob-pd{
      padding-top:0px !important;
      padding-bottom:10px !important;
      }
      span.br-mob {
      display:none !important;
      }
       td.visit2{padding-top:20px !important;}
      td.btn-upgrade{padding-bottom:0px !important;}
      td.height-text div{height:auto !important; min-height:0px !important;}
      br.br-mob {display: none !important;}
      }
    </mj-style>
      <mj-raw>
      <!--[if mso]>
<style type="text/css">
table, td, div {line-height:104% !important;}
