      </mj-column>
    </mj-section>
    <!--Section title-->
    <mj-section>
      <mj-column>
        <mj-text mj-class="head">
          CONFIRMATION DETAILS
        </mj-text>
      </mj-column>
    </mj-section>
    <!--Details-->
    <mj-section padding="20px 20px 0px">
      <mj-column width="30%">
        <mj-text padding="0px 0px 15px" font-weight="bold">
