      </mj-column>
      <mj-column width="70%">
        <mj-text padding="0px 0px 15px">
          @@check_in@@
        </mj-text>
      </mj-column>
    </mj-section>
    <mj-section padding="0px 20px 0px">
      <mj-column width="30%">
        <mj-text padding="0px 0px 15px" font-weight="bold">
          Departure Date
        </mj-text>
      </mj-column>
      <mj-column width="70%">
        <mj-text padding="0px 0px 15px">
          @@check_out@@
        </mj-text>
      </mj-column>
    </mj-section>
    <mj-section padding="0px 20px 0px">
